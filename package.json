    "react-scripts": "2.0.5",
    "react-test-renderer": "16.4.1",
    "redux": "4.0.0",
    "request": "2.87.0",
    "stylelint-config-standard": "18.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "^21.26.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
